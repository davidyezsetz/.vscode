{
/*
	// Place your snippets for Scala here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
	"Image type: T4": {
		"prefix": "imgt4",
		"body": [
			"<img class=\"${2:class}\" alt=\"${3:alt}\" src=\"data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==\"",
			"\tsrcset=\"{{siteImagesUrl}}/media/${1:id}/w288 288w,",
			"\t\t{{siteImagesUrl}}/media/${1:id}/w576 576w,",
			"\t\t{{siteImagesUrl}}/media/${1:id}/w620 620w,",
			"\t\t{{siteImagesUrl}}/media/${1:id}/w1152 1152w\">"
		],
		"description": "All sizes for a t4 type image"
	},
	"Image type: T12": {
		"prefix": "imgt12",
		"body": [
			"<img class=\"${2:class}\" alt=\"${3:alt}\" src=\"data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==\"",
			"\tsrcset=\"{{siteImagesUrl}}/media/${1:id}/w608 608w,",
			"\t\t{{siteImagesUrl}}/media/${1:id}/w992 9924w,",
			"\t\t{{siteImagesUrl}}/media/${1:id}/w1248 1248w,",
			"\t\t{{siteImagesUrl}}/media/${1:id}/w1984 1984w\">"
		],
		"description": "All sizes for a t12 type image"
	}
}